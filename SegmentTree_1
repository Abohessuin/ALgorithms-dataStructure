#include <bits/stdc++.h>
using namespace std;
struct segtree
{
    int siz;
    vector<long long>sums;
    void init(int n)
    {
        siz=1;
        while(siz<n) siz*=2;
        sums.assign(siz*2,0LL);


    }
    void sett(int i,int v,int x,int lx,int rx)
    {
        if(rx-lx==1)
        {
            sums[x]=v;
            return;
        }
        int m=(rx+lx)/2;
        if(i<m)
        {

            sett(i,v,2*x+1,lx,m);
        }
        else
        {
            sett(i,v,2*x+2,m,rx);

        }

         sums[x]=sums[2*x+1]+sums[2*x+2];

    }

    void sett(int i,int v)
    {
         sett(i,v,0,0,siz);
    }
    long long summ(int l,int r,int x,int lx,int rx)
    {
        if(lx>=r||l>=rx) return 0;
        if(lx>=l&&rx<=r) return sums[x];
        int m=(rx+lx)/2;
        long long s1=summ(l,r,2*x+1,lx,m);
        long long s2=summ(l,r,2*x+2,m,rx);
        return s1+s2;
    }
    long long summ(int l,int r)
    {
        summ(l,r,0,0,siz);
    }

};
